# Backend HTTP base URL (Render)
# Local: http://localhost:8080
# Cloud: https://your-backend.onrender.com
NEXT_PUBLIC_BACKEND_URL=http://localhost:8080

# Backend WebSocket URL (Render WSS)
# Local: ws://localhost:8080/ws/live
# Cloud: wss://your-backend.onrender.com/ws/live
NEXT_PUBLIC_WS_URL=ws://localhost:8080/ws/live

# Optional: override Gemini Live model
NEXT_PUBLIC_GEMINI_LIVE_MODEL=gemini-2.5-flash-native-audio-preview-12-2025
